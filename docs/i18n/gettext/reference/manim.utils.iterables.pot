msgstr ""

#: ../../../manim/utils/iterables.py:docstring of manim.utils.iterables.resize_with_interpolation:10
msgid "cycles elements instead of interpolating"
msgstr ""

#: ../../../manim/utils/iterables.py:docstring of manim.utils.iterables.resize_with_interpolation:13
